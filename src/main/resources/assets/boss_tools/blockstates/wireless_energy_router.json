{
  "multipart": [
    {
      "apply": {
        "model": "boss_tools:block/energy_cable_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "boss_tools:block/energy_cable_side",
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "boss_tools:block/energy_cable_side",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "boss_tools:block/energy_cable_side",
        "y": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "boss_tools:block/energy_cable_side",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "boss_tools:block/energy_cable_y_up",
        "uvlock": false
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "boss_tools:block/energy_cable_y_down",
        "uvlock": false
      }
    }
  ]
}